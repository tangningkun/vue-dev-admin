{
    "name": "vue-dev-admin",
    "private": true,
    "version": "1.0.1",
    "type": "module",
    "scripts": {
        "commit": "czg",
        "bootstrap": " pnpm install --ignore-scripts",
        "dev": "npx --max_old_space_size=4096 cross-env BROWSER='chrome' vite",
        "serve": "npm run dev",
        "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
        "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts",
        "build:no-cache": "pnpm clean:cache && npm run build",
        "report": "cross-env REPORT=true npm run build",
        "preview": "npm run build && vite preview",
        "preview:dist": "vite preview",
        "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
        "clean:lib": "rimraf node_modules",
        "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
        "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
        "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
        "lint:ls-lint": "ls-lint",
        "lint:lint-staged": "lint-staged",
        "lint:lint-pretty": "pretty-quick --staged",
        "prepare": "husky install"
    },
    "dependencies": {
        "@ant-design/colors": "^7.0.0",
        "@ant-design/cssinjs": "^1.18.0",
        "@ant-design/icons-vue": "^7.0.1",
        "@commitlint/cli": "^18.4.1",
        "@commitlint/config-conventional": "^18.4.0",
        "@iconify/iconify": "^3.1.1",
        "@vitejs/plugin-vue-jsx": "^3.0.2",
        "@vue/runtime-core": "^3.3.9",
        "@vue/shared": "^3.3.9",
        "@vueuse/core": "^10.6.1",
        "ant-design-vue": "4.x",
        "axios": "^1.6.2",
        "conventional-changelog-cli": "^4.1.0",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.10",
        "devextreme": "23.1",
        "devextreme-vue": "23.1",
        "dotenv": "^16.3.1",
        "exceljs": "^4.4.0",
        "file-saver": "^2.0.5",
        "husky": "^8.0.3",
        "install": "^0.13.0",
        "lint-staged": "^14.0.1",
        "mockjs": "^1.1.0",
        "nprogress": "^0.2.0",
        "path-to-regexp": "^6.2.1",
        "pinia": "^2.1.7",
        "pretty-quick": "^3.1.3",
        "qs": "^6.11.2",
        "resize-observer-polyfill": "^1.5.1",
        "rimraf": "^5.0.5",
        "sortablejs": "^1.15.0",
        "unocss": "^0.57.4",
        "vite-plugin-theme": "^0.8.6",
        "vue": "^3.3.4",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@iconify/json": "^2.2.147",
        "@ls-lint/ls-lint": "^2.2.2",
        "@purge-icons/generated": "^0.10.0",
        "@types/fs-extra": "^11.0.4",
        "@types/inquirer": "^9.0.7",
        "@types/node": "^20.10.4",
        "@types/qs": "^6.9.10",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "@vitejs/plugin-legacy": "^4.1.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "autoprefixer": "^10.4.16",
        "commitizen": "^4.3.0",
        "crypto-js": "^4.2.0",
        "cz-conventional-changelog": "^3.3.0",
        "cz-git": "^1.7.1",
        "czg": "^1.7.1",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-vue": "^9.17.0",
        "esno": "^4.0.0",
        "fs-extra": "^10.0.1",
        "inquirer": "^9.2.12",
        "less": "^4.2.0",
        "lodash-es": "^4.17.21",
        "picocolors": "^1.0.0",
        "postcss": "^8.4.29",
        "postcss-html": "^1.5.0",
        "postcss-less": "^6.0.0",
        "prettier": "^2.8.8",
        "rollup-plugin-visualizer": "^5.9.2",
        "stylelint": "^15.10.3",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-config-recommended": "^13.0.0",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-order": "^6.0.3",
        "typescript": "^5.0.2",
        "unplugin-icons": "^0.18.1",
        "unplugin-vue-components": "^0.25.2",
        "vite": "^4.4.5",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-imagemin": "^0.6.1",
        "vite-plugin-mkcert": "^1.16.0",
        "vite-plugin-mock": "^2.9.6",
        "vite-plugin-purge-icons": "^0.10.0",
        "vite-plugin-pwa": "^0.16.7",
        "vite-plugin-style-import": "^2.0.0",
        "vite-plugin-svg-icons": "^2.0.1",
        "vite-plugin-vue-setup-extend": "^0.4.0",
        "vue-eslint-parser": "^9.3.2",
        "vue-i18n": "^9.7.0",
        "vue-tsc": "^1.8.5",
        "vue-types": "^5.1.1",
        "vuedraggable": "^4.1.0"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,.cs}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
            "prettier --write--parser json"
        ],
        "package.json": [
            "prettier --write"
        ],
        "*.vue": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{scss,less,style,html}": [
            "stylelint --fix",
            "prettier --write"
        ],
        "*.md": [
            "prettier --write"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
